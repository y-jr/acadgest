@model IEnumerable<acadgest.Models.User.AppUser>

<h2>Editar coordenador - @ViewData["coordenation"]</h2>

<form asp-route-id="@ViewData["coordenationId"]" asp-controller="Admin" asp-action="SetCoordinator" method="post">

    <div class="form-group">
        <label for="CoordinatorId">Coordenador</label>
        <select name="CoordinatorId" id="CoordinatorId" class="form-control">
            @foreach (var user in Model)
            {
                <option value="@user.Id">@user.Name</option>
            }
        </select>
    </div>

    <button type="submit" class="btn my-2 btn-primary">Registrar</button>
</form>