@model dynamic

<!DOCTYPE html>
<html>

<head>
    <style>
        body {
            font-family: Arial, sans-serif;
        }

        h1 {
            color: navy;
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        th,
        td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
</head>

<body>
    <h1>Relatório de Dados</h1>
    <p><strong>Nome:</strong> @Model.Name</p>
    <p><strong>Idade:</strong> @Model.Age</p>
    <p><strong>Gênero:</strong> @Model.Gender</p>
</body>

</html>
